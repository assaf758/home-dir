#!/bin/sh
wingo --replace 2>/dev/null &
sleep 4
wingo-cmd 'RenameWorkspace 0 "Local"'
wingo-cmd 'RenameWorkspace 1 "Redirect"'
wingo-cmd 'RenameWorkspace 2 "WAF2"'
wingo-cmd 'RenameWorkspace 3 "Devices"'
wingo-cmd 'RenameWorkspace 4 "5"'
wingo-cmd 'RenameWorkspace 5 "6"'
xrdb -merge ~/.Xresources
xbindkeys 
kbd_home.sh
tmuxinator misc 
# xfce4-terminal -e "tmux attach-session -t misc"  &
#tmux attach-session -t misc"
