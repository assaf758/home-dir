#/bin/sh

prefix=$(printf '%(%s)T\n' -1)

for i in {0..7} 
do
    wingo-cmd "RenameWorkspace ${i} \"${prefix}-${i}\""
done

declare -a ws_list=("aux-laptop" 
                "ha" "staging"
                "misc"
                )
i=0
for ws in "${ws_list[@]}"
do
    wingo-cmd "RenameWorkspace ${i} \"${ws}\""
    ((i++))
done
