[user]
	name = assaf758
	email = assaf758@hlinux.hlinux
[reviewboard]
	url = http://cmp-cdr-srv1.cmpsys.com
[color]
	diff = auto
	status = auto
	branch = auto
[alias]
    meld = !git-meld.pl
    ci = commit
    co = checkout
    st = status
    br = branch 
    df = diff
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
    rso = remote show origin

    # Show files ignored by git:
    ign = ls-files -o -i --exclude-standard

[merge]
	tool = git-meld.pl
[diff]
[gui]
	recentrepo = U:/test/gitflow2.git
[credential]
	helper = cache --timeout=3600
[core]
    autocrlf = false
    safecrlf = true
    eol       = lf
[push]
	default = simple
